{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./src/index.ts"
  ],
  "theme": "github-wiki",
  "out": "docs",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-github-wiki-theme"
  ],
  "excludePrivate": true,
  "categorizeByGroup": true,

  "entryDocument": "src/README.md",
  "hideBreadcrumbs": false,
  "hideInPageTOC": false,
  "hideMembersSymbol": false,
  "namedAnchors": true,
  "preserveAnchorCasing": false
}
